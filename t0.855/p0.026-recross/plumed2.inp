UNITS NATURAL

VOLUME LABEL=vol
CUSTOM ARG=vol FUNC=x/17576 PERIODIC=NO LABEL=molvol

COORDINATIONNUMBER ...
  LABEL=coord
  SPECIES=1-17576
  SWITCH={RATIONAL R_0=1.6 D_MAX=2.5}
  LESS_THAN={RATIONAL R_0=5 D_MAX=7.5 NN=12}
  LOWMEM
... COORDINATIONNUMBER

COMMITTOR ...
  ARG=coord.lessthan
  STRIDE=1
  BASIN_LL1=0
  BASIN_UL1=420
  BASIN_LL2=420
  BASIN_UL2=17576
  NOSTOP
  FILE=commit
... COMMITTOR

FLUSH STRIDE=2000
PRINT ARG=molvol,coord.lessthan STRIDE=200 FILE=colvar

